<!DOCTYPE html>
<html lang="en" ng-app="GZ" ng-strict-di>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="@Url.Content("~/favicon.ico")" rel="shortcut icon" type="image/x-icon" />
    <link href="@Url.Content("~/apple-touch-icon.png")" rel="apple-touch-icon" />
    <link href="@Url.Content("~/apple-touch-icon.png")" rel="apple-touch-icon-precomposed" />

    <title ng-if="!documentTitle">Greenzorro</title>
    <title ng-if="documentTitle">{{documentTitle}}</title>
    <base href="/" />

    @Styles.Render("~/css/bootstrap")
    @Styles.Render("~/css/fa")
    @*@Styles.Render("~/css/app_old")*@
    @Styles.Render("~/css/app")
</head>
<body ng-cloak>
    @{Html.RenderAction("Header", "Home");}
    
    @RenderBody()

    @Html.Partial("_Footer")

    @Scripts.Render("~/js/jquery")
    @Scripts.Render("~/js/plugins")
    @Scripts.Render("~/js/bootstrap")
    @Scripts.Render("~/js/angular")
    @Scripts.Render("~/js/modules")
    @Scripts.Render("~/js/app")
</body>
</html>
